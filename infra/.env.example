# Example environment variables for development
# Copy this to .env and update with your values

# Zahara.ai Branding
APP_NAME=Zahara.ai API
APP_DESCRIPTION=Zahara.ai - Intelligent AI Platform API
COMPANY_NAME=Zahara.ai
COMPANY_URL=https://zahara.ai

# Enable development features (0 or 1)
ENABLE_DEV_PAGES=0

# Database configuration
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/postgres

# Redis configuration  
REDIS_URL=redis://redis:6379

# Qdrant configuration
QDRANT_URL=http://qdrant:6333

# API Keys (replace with real values)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here
OPENROUTER_API_KEY=your_openrouter_key_here

# Security
SECRET_KEY=dev_secret_key_change_in_production

# Debug mode
DEBUG=true

# Flowise Configuration (Option A Fork Integration)
FLOWISE_COMMIT_SHA=af1464f7c2b9a608a2763f5d696d6670e8f51a7e
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=admin123